<?xml version="1.0" encoding="utf-8" ?>
<configuration>

  <configSections>
    <section name="WebDriver" type="System.Configuration.NameValueSectionHandler" />
    <section name="Export" type="System.Configuration.NameValueSectionHandler" />
    <section name="DataModel" type="System.Configuration.NameValueSectionHandler" />
    <section name="ScreenShots" type="System.Configuration.NameValueSectionHandler" />
    <section name="Wait" type="System.Configuration.NameValueSectionHandler" />
    <section name="Proxy" type="System.Configuration.NameValueSectionHandler" />
    <section name="Exceptions" type="System.Configuration.NameValueSectionHandler" />
  </configSections>

  <WebDriver>
    <add key="DriverDirectory" value="WebDrivers"></add>
    <add key="ActiveDriver" value="0"></add>
    <add key="0" value="PhantomJs"></add>
    <add key="1" value="Chrome"></add>
    <add key="2" value="Firefox"></add>
    <add key="ImplicitWait" value="2.95"></add>
    <add key="PageTimeOut" value="60"></add>
    <add key="ScriptTimeOut" value="60"></add>
  </WebDriver>

  <Wait>
    <add key="PageLoad" value="60"></add>
    <add key="Element" value="30"></add>
    <add key="Navigation" value="20"></add>
  </Wait>

  <Export>
    <add key="ExportDirectory" value="c:\DataPullerExport"></add>
  </Export>

  <DataModel>
    <add key="DataModelDirectory" value="DataPullerSiteModels"></add>
  </DataModel>

  <ScreenShots>
    <add key="ScreenShotDirectory" value="ScreenShots"></add>
  </ScreenShots>

  <Exceptions>
    <add key="ExceptionsDirectory" value="Exceptions"></add>
  </Exceptions>
    
  <Proxy>
    <add key="ProxyDirectory" value="Proxy\\Tor"></add>
    <add key="IP" value="127.0.0.1"></add>
    <add key="Port" value="9150"></add>
    <add key="Type" value="socks5"></add>
    <add key="Timeout" value="5000"></add>
  </Proxy>

  
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5" />
  </startup>
  <appSettings>
    <add key="DBThreshold" value="50" />
    <add key="ConnectionString" value ="Data Source=.;Integrated Security=True;"/>
    <add key="TemplatePath" value ="C:\Templates"/>
  </appSettings>
</configuration>